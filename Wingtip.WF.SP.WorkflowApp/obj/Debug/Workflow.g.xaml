              </InArgument>
            </p:SingleTask.AssignedTo>
            <p:SingleTask.Body>
              <InArgument x:TypeArguments="x:String">
                <mca:CSharpValue x:TypeArguments="x:String">ApprovalRequestMessage</mca:CSharpValue>
              </InArgument>
            </p:SingleTask.Body>
            <p:SingleTask.DueDate>
