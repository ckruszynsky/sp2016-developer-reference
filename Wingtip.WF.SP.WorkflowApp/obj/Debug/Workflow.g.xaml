                </FlowStep>
              </FlowSwitch.Default>
              <FlowSwitch.Expression>
                <mca:CSharpValue x:TypeArguments="x:Int32">ApprovalTaskOutcome</mca:CSharpValue>
              </FlowSwitch.Expression>
              <FlowStep x:Key="0" x:Name="__ReferenceID3">
                <p:WriteToHistory DisplayName="Write Approved" Message="Task Approved" />
              </FlowStep>
            </FlowSwitch>
          </FlowStep.Next>
        </FlowStep>
